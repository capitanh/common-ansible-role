[color]
  ui = true
  interactive = auto
[color "branch"]
  current = blue
  local = yellow
  remote = green
[core]
  editor = micro
  pager = delta
  excludesFile = ~/.gitignore
[alias]
  co = checkout
  br = branch
  st = status
[delta]
  side-by-side = true
[interactive]
  diffFilter = delta --color-only --features=interactive
